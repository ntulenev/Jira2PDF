{
  "Jira": {
    "BaseUrl": "",
    "Email": "",
    "ApiToken": "",
    "MaxResultsPerPage": 100,
    "DefaultPdfPath": "jql-report.pdf",
    "Reports": [
      {
        "Name": "Done Bugs",
        "Jql": "project = AAA AND issuetype = Bug AND status = Done ORDER BY updated DESC",
        "OutputFields": [ "key", "summary", "status", "assignee", "updated" ],
        "PdfReportName": "AAA_Done_Bugs"
      },
      {
        "Name": "Active Work",
        "Jql": "project = AAA AND status NOT IN (Done, Rejected) ORDER BY priority DESC, updated DESC",
        "OutputFields": [ "key", "issuetype", "status", "assignee", "created", "summary" ],
        "PdfReportName": "AAA_Active_Work"
      }
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "System.Net.Http.HttpClient": "Warning",
      "Microsoft": "Warning"
    }
  }
}
